.content {
  width: 90%;
  height: 350rpx;
  margin-top: 20rpx;
  margin-left: 5%;
  position: relative;
}
.content image {
  position: absolute;
}
.content .conLogo {
  width: 100%;
  height: 100%;
  z-index: -1;
}
.content .conImg {
  width: 100rpx;
  height: 100rpx;
  top: 30rpx;
  left: 30rpx;
}
.content text {
  font-size: 0.9rem;
  color: #fff;
  position: absolute;
  left: 25%;
}
.content .conTextName {
  top: 45rpx;
}
.content .conTextMoney {
  top: 90rpx;
  font-size: 0.8rem;
}
.content .conIcon {
  width: 25rpx;
  height: 35rpx;
  top: 65%;
  left: 60rpx;
}
.content navigator {
  position: absolute;
  top: 65%;
  left: 100rpx;
  font-size: 0.8rem;
  color: #fff;
}
